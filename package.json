{
  "name": "browserify-phaser",
  "version": "0.1.0",
  "description": "A template to get you started using Phaser with Browserify!",
  "scripts": {
    "start": "beefy src/game.js:bundle.js --live --open",
    "bundle": "browserify src/game.js -o bundle.js ",
    "deploy": "gulp deploy",
    "build": "gulp build"
  },
  "dependencies": {
    "beefy": "~2.1.1",
    "browserify": "~7.0.0",
    "browserify-fs": "^1.0.0",
    "del": "^1.1.1",
    "depot": "^0.1.6",
    "eventemitter2": "^0.4.14",
    "minimongo": "^3.2.5",
    "mixin-object": "^0.1.1",
    "phaser": "~2.2.1",
    "treo": "^0.4.1",
    "vinyl-paths": "^1.0.0"
  },
  "devDependencies": {
    "gulp": "3.8.8",
    "gulp-gh-pages": "^0.4.0",
    "vinyl-source-stream": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OttoRobba/browserify-phaser.git"
  },
  "keywords": [
    "phaser",
    "template",
    "game",
    "template",
    "browserify"
  ],
  "author": "OttoRobba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OttoRobba/browserify-phaser/issues"
  }
}
